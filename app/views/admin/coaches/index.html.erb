<p id="notice"><%= notice %></p>

<h1>Listing Admin Coaches</h1>

<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Coach name</th>
      <th>Coach status</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_coaches.each do |admin_coach| %>
      <tr>
        <td><%= admin_coach.coach_name %></td>
        <td><%= admin_coach.status %></td>
        <td><%= link_to 'Show', admin_coach %></td>
        <td><%= link_to 'Edit', edit_admin_coach_path(admin_coach) %></td>
        <!-- <td><%= link_to 'Destroy', admin_coach, method: :delete, data: { confirm: 'Are you sure?' } %></td> -->
      </tr>
    <% end %>
  </tbody>
</table>

<br>

